<Root>
	<MediaCasters>
		<Connections>
			<ProcessorCount>4</ProcessorCount>
			<SocketConfiguration>
				<ReuseAddress>true</ReuseAddress>
				<ReceiveBufferSize>65536</ReceiveBufferSize>
				<SendBufferSize>32768</SendBufferSize>
				<KeepAlive>true</KeepAlive>
				<!-- <TrafficClass>0</TrafficClass> -->
				<!-- <OobInline>false</OobInline> -->
				<!-- <SoLingerTime>-1</SoLingerTime> -->
				<!-- <TcpNoDelay>false</TcpNoDelay> -->
			</SocketConfiguration>
		</Connections>
		<MediaCaster>
			<Name>shoutcast</Name>
			<Description>ShoutCast</Description>
			<StreamType>shoutcast-buffer</StreamType>
			<BaseClass>com.wowza.wms.mediacaster.shoutcast.ShoutCastReceiver</BaseClass>
			<ConnectionTimeout>4000</ConnectionTimeout>
			<KeepAliveTime>10000</KeepAliveTime>
			<Properties>
				<Property>
					<Name>streamTimeout</Name>
					<Value>12000</Value>
					<Type>Integer</Type>
				</Property>
				<Property>
					<Name>characterEncoding</Name>
					<Value>8859_1</Value>
					<!-- <Value>UTF-8</Value> -->
				</Property>
				<Property>
					<Name>isMediaCasterVideo</Name>
					<Value>false</Value>
					<Type>Boolean</Type>
				</Property>
			</Properties>
		</MediaCaster>
